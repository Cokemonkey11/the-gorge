scope Farm initializer i
	function c takes nothing returns nothing
		player p = GetOwningPlayer(GetKillingUnit())
		integer id = GetPlayerId(p)
		if id>1 then
			Game.farmIncrement(id)
		endif
	endfunction

	function i takes nothing returns nothing
		registerPlayerUnitEvent(EVENT_PLAYER_UNIT_DEATH,function c)
	endfunction
endscope

library FloatText requires Game
	public function reviveNumbers takes real x, real y, string s, integer pID returns nothing
		texttag t=CreateTextTag()
        SetTextTagFadepoint(t,3)
        SetTextTagLifespan(t,4)
        SetTextTagPermanent(t,false)
        SetTextTagPos(t,x,y,100)
        SetTextTagText(t,s,.02)
        SetTextTagVelocity(t,0,.05)
		if Game.localPlayer == Game.players[pID] then
			SetTextTagVisibility(t,true)
		endif
	end
endlibrary

@compiletime function create_crewmembers()
    CREW_PROPS.forEach() (LHMEntry<Crewmember, CrewmemberProps> kv) ->
        new CrewmemberDef(
            kv.value.id,
            kv.value.idpp,
            kv.value.price,
            kv.value.name,
            kv.value.posX,
            kv.value.posY,
            kv.value.damage,
            kv.value.aps,
            kv.value.range,
            kv.value.icon,
            kv.value.projectileScale,
            kv.value.soundSet,
            kv.value.shouldHitStructures
        )


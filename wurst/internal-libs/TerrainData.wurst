		static constant EASTX               =    -1. * WESTX
		static constant POS_STONE1          = vec2( 380.,  1030.)
		static constant POS_STONE2          = vec2(-140., -1020.)
		static constant SAFE_X              = -8064.
		static constant SAFE_Y              =  3970.
		static constant STONE_SPIDER_X      =   -50.
		static constant STONE_SPIDER_Y      = -4480.
		static constant STONE_SPIDER_RADIUS =   128.

		// Corners of a region that contains the central island, plus towers.
		static constant CENTRAL_MINX     =  -770.
		static constant CENTRAL_MINY     = -1950.
		static constant CENTRAL_MAXX     =  1060.
		static constant CENTRAL_MAXY     =  1870.

		// Used by FillToxicSack (Trigger).
		static rect utilityWest
		static rect utilityEast
		static rect spider


	init
